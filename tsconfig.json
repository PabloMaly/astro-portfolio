{
    "extends": "astro/tsconfigs/strict",
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist"
    ],
    "compilerOptions": {
        "types": [
            "@types/node",
            "tailwindcss"
        ],
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "../*": [
                "src/*"
            ]
        }
    }
}